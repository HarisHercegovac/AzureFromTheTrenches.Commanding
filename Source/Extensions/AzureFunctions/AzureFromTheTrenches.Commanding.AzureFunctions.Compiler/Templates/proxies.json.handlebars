{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        {{#each .}}
        {{#if @index}},{{/if}}"{{Name}}Proxy": {
            "matchCondition": {
                "route": "{{Route}}",
                "methods": [
                    {{{httpVerbs}}}
                ]
            },
            "backendUri": "https://%HostDomain%/api/{{Name}}"
        }
        {{/each}}
    }
}
